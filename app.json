{
  "expo": {
    "name": "1stapp",
    "slug": "1stapp",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./app/assets/logo-red.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./app/assets/logo-red.png"
    },
    "description": "This is my first expo app\n"
  }
}
